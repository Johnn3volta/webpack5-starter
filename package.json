{
  "name": "template-starter",
  "version": "1.0.0",
  "description": "Starter template for development",
  "main": "index.js",
  "author": "johnn3volta",
  "license": "MIT",
  "scripts": {
    "dev": "webpack serve -c ./configs/webpack/dev.js",
    "build": "webpack -c ./configs/webpack/prod.js"
  },
  "browserslist": [
    "> .5%, not dead"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/plugin-transform-spread": "^7.13.0",
    "@babel/preset-env": "^7.13.15",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "imagemin-webpack-plugin": "^2.4.2",
    "jpegtran": "^2.0.0",
    "mini-css-extract-plugin": "^1.5.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.12",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "pug-html-loader": "^1.1.5",
    "sass": "^1.32.11",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.35.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-image-resize-loader": "^4.0.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "html-webpack-pug-plugin": "^3.0.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.6"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "plugins": [
      "babel",
      "import",
      "prettier"
    ],
    "globals": {
      "window": true,
      "document": true,
      "jQuery": true,
      "$": true,
      "grecaptcha": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "printWidth": 100,
          "singleQuote": true,
          "trailingComma": "all",
          "semi": true,
          "bracketSpacing": true,
          "tabWidth": 2
        }
      ],
      "no-unused-expressions": [
        "error",
        {
          "allowTernary": true
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true,
          "optionalDependencies": false,
          "peerDependencies": false
        }
      ],
      "camelcase": [
        "error",
        {
          "ignoreDestructuring": true,
          "properties": "never"
        }
      ]
    }
  }
}
